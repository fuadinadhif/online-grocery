{
  "name": "api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "npx prisma generate && npx tsc",
    "check-types": "npx tsc --noEmit",
    "dev": "NODE_ENV=development npx tsx --watch src/app.ts",
    "lint": "npx eslint",
    "start": "npm run build && NODE_ENV=production node dist/app.js"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "globals": "^15.15.0",
    "prisma": "^6.3.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.2"
  }
}
